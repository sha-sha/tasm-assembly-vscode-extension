{
  "name": "tasm-ext",
  "displayName": "tasm-ext",
  "description": "TASM",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:tasm"
  ],
  "main": "./client/out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "tasm",
        "aliases": [
          "tasm",
          "tasm"
        ],
        "extensions": [
          ".asm",
          ".asx"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "tasm",
        "scopeName": "source.tasm",
        "path": "./syntaxes/tasm.tmLanguage.json"
      }
    ]
  },
  "dependencies": {
    "vscode-languageclient": "^9.0.1"
  },
  "devDependencies": {
    "@types/vscode": "^1.100.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.10.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.8.3",
    "@vscode/test-electron": "^2.3.4"
  }
}